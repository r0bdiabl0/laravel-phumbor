{
    "name": "r0bdiablo/laravel5-phumbor",
    "description": "Laravel 5/6 package adding support for the Phumbor PHP library for Thumbor.",
	"homepage": "https://github.com/r0bdiablo/laravel5-phumbor",
	"license": "MIT",
	"keywords": ["laravel", "phumbor", "thumbor"],
    "authors": [
        {
            "name": "Robert Pettique",
            "email": "robert@rkcreative.co"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/support": "5.*|6.*",
        "99designs/phumbor": "^1.2"
    },
    "require-dev": {
        "graham-campbell/testbench": "^4.0"
    },
    "autoload": {
        "psr-0": {
            "R0bdiabl0\\Laravel5Phumbor": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "R0bdiabl0\\Laravel5Phumbor\\Laravel5PhumborServiceProvider"
            ],
            "aliases": {
                "Debugbar": "R0bdiabl0\\Laravel5Phumbor\\Phumbor"
            }
        }
    }
}
